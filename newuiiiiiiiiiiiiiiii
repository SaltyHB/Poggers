            Label2.Name = "Label"
            Label2.Parent = ScrollTab
            Label2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Label2.BackgroundTransparency = 1.000
            Label2.Size = UDim2.new(0, 455, 0, 57)
            Label2.Font = Enum.Font.GothamSemibold
            Label2.TextColor3 = Color3.fromRGB(225, 225, 225)
            Label2.TextSize = 11.000
            Label2.Text = text
            Label2.TextXAlignment = Enum.TextXAlignment.Left

            PaddingLabel2.PaddingLeft = UDim.new(0,10)
            PaddingLabel2.Parent = Label
            PaddingLabel2.Name = "PaddingLabel"

            local MainCorner = Instance.new("UICorner")
            MainCorner.Name = "MainCorner"
            MainCorner.CornerRadius = UDim.new(0, 5)
            MainCorner.Parent = Label2
            
            local DropdownStroke = Instance.new("UIStroke")

