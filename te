--// Valentine Shop
local textlab = valentines:addLabel("")
local textlab2 = valentines:addLabel("")

spawn(function()
    while wait(.1) do
        local valen = game:GetService("Players").LocalPlayer.PlayerGui.Main.CelebrationShop
        textlab:UpdateLabel(tostring(valen.Bar.TextLabel.Text))
        textlab2:UpdateLabel(tostring(valen.Bar.TextLabel2.Text))
    end
end)
local store = unpack(game.ReplicatedStorage.Remotes.Celebration:InvokeServer("GetStore"))
for i,v in pairs(store) do
    valentines:addButton(tostring(v.Name).." [ "..tostring(v.Price).." Hearts ]", "", function()
        game:GetService("ReplicatedStorage").Remotes.Celebration:InvokeServer("Purchase", v.Name)
        print("Purchase "..v.Name)
    end)
end

