    secondQuest:addToggle('Toggle', 'Auto Trade Bone', 'The king', false, function(autotradebonef)
        AutoTradeBone = autotradebonef
    end)
    spawn(function()
        while task.wait(.1) do
            if AutoTradeBone then   
                pcall(function() 
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones","Buy",1,1)
                end)
            end
        end
    end)

