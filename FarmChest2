if _G.FarmChest then repeat task.wait(4)until game:IsLoaded()local a=game.PlaceId;local b={}local c=""local d=os.date("!*t").hour;local e,f=pcall(function()b=game:GetService("HttpService"):JSONDecode(readfile("NotSameServers.json"))end)if not e then b={d}writefile("NotSameServers.json",game:GetService("HttpService"):JSONEncode(b))end;local function g()local h;if c==""then h=game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..a.."/servers/Public?sortOrder=Asc&limit=100"))else h=game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..a.."/servers/Public?sortOrder=Asc&limit=100&cursor="..c))end;if h.nextPageCursor then c=h.nextPageCursor end;for i,j in ipairs(h.data)do if j.playing<j.maxPlayers then local k=false;for i,l in ipairs(b)do if j.id==tostring(l)then k=true;break end end;if not k then table.insert(b,j.id)pcall(function()writefile("NotSameServers.json",game:GetService("HttpService"):JSONEncode(b))game:GetService("TeleportService"):TeleportToPlaceInstance(a,j.id,game.Players.LocalPlayer)end)task.wait(4)end end end end;local function m()while wait()do pcall(g)end end;task.spawn(function()while wait(0.5)do pcall(function()for i,n in ipairs(game.CoreGui:GetDescendants())do if n:IsA("TextLabel")and n.Name:find("ErrorMessage")and n.Text:find("Security kick")then m()end end end)end end)AllowRunService=true;task.spawn(function()while AllowRunService and task.wait()do local o=game.Players.LocalPlayer;local p=o.Character or o.CharacterAdded:Wait()if p then local q=p:FindFirstChild("CrewBBG",true)if q then q:Destroy()end;local r=p:FindFirstChild("Humanoid")if r then local s=game:GetService("Workspace").ChestModels;local t=s:FindFirstChild("SilverChest")or s:FindFirstChild("DiamondChest")or s:FindFirstChild("GoldChest")or s:FindFirstChild("Mirage")or s:FindFirstChild("Fragment")if t and t:FindFirstChild("RootPart")then p:PivotTo(t.RootPart:GetPivot())else m()break end end end end end)end
